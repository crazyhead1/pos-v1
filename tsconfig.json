{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports" : true,
      "esModuleInterop" : true,
      "baseUrl": "src",
      "paths": {
        "assets/*": ["assets/*"]
      }
    },
    "include": [ "src/assets", "src/custom.d.ts"],
  }